    "prettier": "^2.8.1",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-pwa": "^0.14.0"
