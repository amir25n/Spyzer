    "format": "run-s format:prettier format:eslint format:stylelint",
    "format:eslint": "eslint '**/*.ts' --fix",
    "format:stylelint": "stylelint '**/*.html' '**/*.css' '**/*.ts' --fix",
    "format:prettier": "prettier \"**/*.{html,json,md,ts,css}\" --ignore-path ./.eslintignore --write"
